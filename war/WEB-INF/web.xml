<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Demo</servlet-name>
		<servlet-class>com.demo.DemoServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Demo</servlet-name>
		<url-pattern>/demo</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Session</servlet-name>
		<servlet-class>com.demo.SessionServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Session</servlet-name>
		<url-pattern>/session</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Session1</servlet-name>
		<servlet-class>com.demo.SessionCreate</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Session1</servlet-name>
		<url-pattern>/sessioncreate</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Fetch</servlet-name>
		<servlet-class>com.demo.FetchServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Fetch</servlet-name>
		<url-pattern>/fetch</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Validate</servlet-name>
		<servlet-class>com.demo.ValidateServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Validate</servlet-name>
		<url-pattern>/validate</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
